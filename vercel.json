{
  "outputDirectory": "website/client/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "includeFiles": [
        "data/storage.db",
        "website/**/*.py",
        "chess_llm_eval/**/*.py"
      ],
      "excludeFiles": [
        "**/__pycache__/**",
        "**/*.pyc"
      ]
    }
  },
  "env": {
    "CHESS_DB_IMMUTABLE": "true"
  }
}
